{% extends "item_source.tera" %}
{% block elements %}
{%- for element in data.elements %}
{%- if element.type == "Custom" %}
!procedure {{ element.procedure_name }}($id, $name, $tech="")
  C4Boundary('{{ element.properties.type }}', $id, $name, $tech)
!endprocedure
{%- endif %}
{% endfor -%}
{% endblock elements %}
